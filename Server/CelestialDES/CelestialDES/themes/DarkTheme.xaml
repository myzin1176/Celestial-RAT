    <!-- Background Colors -->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#85000000"/>
    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#95000000"/>
    <SolidColorBrush x:Key="ContextMenuBackgroundColor" Color="#951C1C1C"/>

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#3772FF"/>

    <!-- Typograhy Colors -->
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
